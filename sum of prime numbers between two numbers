def prime(num):
    if num <= 1:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True
m = int(input("enter number m : "))
n = int(input("enter number n : "))
s = 0
if m > n:
    print("0")
else:
    for i in range(m, n + 1):
        if prime(i):
            s = s+i
    print(s)
